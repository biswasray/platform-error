{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": [
      true,
      "warning"
    ],
    "forin": false
  },
  "linterOptions": {
    "exclude": [
      "dist/**",
      "build/**",
      "config/**",
      "coverage/**",
      "node_modules/**"
    ]
  }
}